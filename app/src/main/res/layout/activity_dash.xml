<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_test_container"
    tools:context=".ui.dashboard.DashActivity">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:foregroundGravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:foregroundGravity="center">

            <!--payment background-->
            <ImageView
                android:id="@+id/payment_rect"
                android:layout_width="155dp"
                android:layout_height="154dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="50dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/white_circle_black_outline"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/resume_rect"
                app:layout_constraintTop_toTopOf="parent" />
            <!--payments image-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:gravity="center"
                android:text="@string/payments"
                android:textColor="@android:color/black"
                android:textSize="12sp"
                android:textStyle="bold"
                app:fontFamily="monospace"
                app:layout_constraintBottom_toTopOf="@+id/talent_image"
                app:layout_constraintEnd_toEndOf="@+id/talent_image"
                app:layout_constraintHorizontal_bias="0.48"
                app:layout_constraintStart_toStartOf="@+id/talent_image"
                app:layout_constraintTop_toTopOf="@+id/payment_rect" />

            <ImageView
                android:id="@+id/talent_image"
                android:layout_width="76dp"
                android:layout_height="76dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/payments"
                app:layout_constraintBottom_toBottomOf="@+id/payment_rect"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/payment_rect"
                app:layout_constraintTop_toTopOf="@+id/payment_rect" />

            <!--resume background-->
            <ImageView
                android:id="@+id/resume_rect"
                android:layout_width="155dp"
                android:layout_height="155dp"
                android:layout_marginTop="50dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/white_circle_black_outline"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <!--resume image-->
            <TextView
                android:id="@+id/user_rect_text_view"
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:gravity="center"
                android:text="@string/talent_resume"
                android:textStyle="bold"
                android:textColor="@android:color/black"
                android:textSize="12sp"
                app:fontFamily="monospace"
                app:layout_constraintBottom_toTopOf="@+id/resume_image"
                app:layout_constraintEnd_toEndOf="@+id/resume_image"
                app:layout_constraintStart_toStartOf="@+id/resume_image"
                app:layout_constraintTop_toTopOf="@+id/resume_rect" />

            <ImageView
                android:id="@+id/resume_image"
                android:layout_width="76dp"
                android:layout_height="76dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/talent_resume"
                app:layout_constraintBottom_toBottomOf="@+id/resume_rect"
                app:layout_constraintEnd_toEndOf="@+id/resume_rect"
                app:layout_constraintHorizontal_bias="0.422"
                app:layout_constraintStart_toStartOf="@+id/resume_rect"
                app:layout_constraintTop_toTopOf="@+id/resume_rect"
                app:layout_constraintVertical_bias="0.493" />


            <!--Grades-->
            <ImageView
                android:id="@+id/grades_rect"
                android:layout_width="155dp"
                android:layout_height="155dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="50dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/white_circle_black_outline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/events_rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/resume_rect"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:gravity="center"
                android:text="@string/grades"
                android:textColor="@android:color/black"
                android:textStyle="bold"
                android:textSize="12sp"
                app:fontFamily="monospace"
                app:layout_constraintBottom_toTopOf="@+id/grades_image"
                app:layout_constraintEnd_toEndOf="@+id/grades_image"
                app:layout_constraintHorizontal_bias="0.606"
                app:layout_constraintStart_toStartOf="@+id/grades_image"
                app:layout_constraintTop_toTopOf="@+id/grades_rect"
                app:layout_constraintVertical_bias="0.333" />

            <ImageView
                android:id="@+id/grades_image"
                android:layout_width="76dp"
                android:layout_height="76dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/grades"
                app:layout_constraintBottom_toBottomOf="@+id/grades_rect"
                app:layout_constraintEnd_toEndOf="@+id/grades_rect"
                app:layout_constraintHorizontal_bias="0.422"
                app:layout_constraintStart_toStartOf="@+id/grades_rect"
                app:layout_constraintTop_toTopOf="@+id/grades_rect"
                app:layout_constraintVertical_bias="0.506" />

            <!--Events-->
            <ImageView
                android:id="@+id/events_rect"
                android:layout_width="155dp"
                android:layout_height="155dp"
                android:layout_marginBottom="50dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/white_circle_black_outline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:gravity="center"
                android:text="@string/events"
                android:textColor="@android:color/black"
                android:textStyle="bold"
                android:textSize="12sp"
                app:fontFamily="monospace"
                app:layout_constraintBottom_toTopOf="@+id/events_image"
                app:layout_constraintEnd_toEndOf="@+id/events_image"
                app:layout_constraintStart_toStartOf="@+id/events_image"
                app:layout_constraintTop_toTopOf="@+id/events_rect" />

            <ImageView
                android:id="@+id/events_image"
                android:layout_width="76dp"
                android:layout_height="76dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_toEndOf="@id/rect_student"
                android:layout_toRightOf="@id/rect_student"
                android:contentDescription="@string/background"
                android:src="@drawable/events"
                app:layout_constraintBottom_toBottomOf="@+id/events_rect"
                app:layout_constraintEnd_toEndOf="@+id/events_rect"
                app:layout_constraintHorizontal_bias="0.549"
                app:layout_constraintStart_toStartOf="@+id/events_rect"
                app:layout_constraintTop_toTopOf="@+id/events_rect"
                app:layout_constraintVertical_bias="0.506" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>